@{
    ViewData["Title"] = "Home Page";
}

@model List<AboutMe.Models.Article>

@if (Model != null && Model.Count > 0)
{
    <div id="blog-block">
        <h1>Останні пости</h1>
        <ul>
            @foreach (var post in Model)
            {
                <li>
                    <h2>@post.Title</h2>
                    <p>@post.Content</p>
                </li>
            }
        </ul>
    </div>
}
else
{
    <p>No records to display</p>
}
